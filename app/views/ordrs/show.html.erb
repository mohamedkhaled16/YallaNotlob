        <div class="contact-form">

          <p id="notice"><%= notice %></p>

          <p>
            <strong>Otype:</strong>
            <%= @ordr.otype %>
          </p>

          <p>
            <strong>Ofrom:</strong>
            <%= @ordr.ofrom %>
